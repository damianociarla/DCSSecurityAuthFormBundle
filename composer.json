{
  "name": "dcs/security-auth-form-bundle",
  "type": "symfony-bundle",
  "description": "The DCSSecurityAuthFormBundle provides an authentication system using the login form",
  "keywords": ["Security","Form","Login"],
  "license": "MIT",
  "authors": [
    {
      "name": "Damiano Ciarla",
      "email": "damiano.ciarla@gmail.org"
    }
  ],
  "require": {
    "php": ">=5.3.9",
    "symfony/form": "~2.8|~3",
    "symfony/security-bundle": "~2.8|~3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "dcs/security-core-bundle": "~1.0@dev",
    "symfony/framework-bundle": "~2.8|~3",
    "satooshi/php-coveralls": "~1.0",
    "phpunit/phpcov": "3.*"
  },
  "autoload": {
    "psr-4": {
      "DCS\\Security\\Auth\\FormBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DCS\\Security\\Auth\\FormBundle\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}